#include <stdio.h>

int main() {
    printf("Таблица умножения:\n");
    printf("   ");
    for (int o = 1; o <= 9; o++) {
        printf("%3d", o);
    }
    printf("\n");

    for (int i = 1; i <= 9; i++) {
        printf("%3d", i);
        for (int j = 1; j <= 9; j++) {
            int result = i * j;
            printf("%3d", result);
        }
        printf("\n");
    }
    return 0;
}
